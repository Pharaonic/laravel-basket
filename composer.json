{
    "name": "pharaonic/laravel-basket",
    "description": "Shopping cart management",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "ecommerce",
        "cart",
        "basket"
    ],
    "authors": [
        {
            "name": "Moamen Eltouny (Raggi)",
            "email": "support@raggitech.com"
        },
        {
            "name": "Karim Mohamed (Komtcho)",
            "email": "komicho1996@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=8.0",
        "pharaonic/laravel-helpers": ">=2.4.3"
    },
    "autoload": {
        "psr-4": {
            "Pharaonic\\Laravel\\Basket\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaonic\\Laravel\\Basket\\BasketServiceProvider"
            ],
            "aliases": {
                "Basket": "Pharaonic\\Laravel\\Basket\\Facades\\Basket"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}